{
  "name": "kubernetes-jenkins",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/scriptcamp/kubernetes-jenkins",
  "author": "Scott Key <scottdkey@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:docker": "docker build -t scottdkey/basic-api:$(echo $npm_package_version) .",
    "build:docker-push": "docker push scottdkey/basic-api:$(echo $npm_package_version)",
    "tsc": "tsc",
    "watch:tsc": "tsc -w",
    "watch:node": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "dev": "run-p watch:*",
    "build": "run-s build:*",
    "test": "mocha -r ts-node/register tests/**/*.test.ts --exit"
  },
  "dependencies": {
    "koa": "^2.13.1",
    "koa-router": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/koa": "^2.13.3",
    "@types/koa-router": "^7.4.2",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "jest": "^27.0.4",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0"
  }
}